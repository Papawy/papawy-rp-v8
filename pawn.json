{
	"user": "Papawy",
	"repo": "papawy-rp-v8",
	"entry": "pwyrp.pwn",
	"output": "pwyrp.amx",
	"dependencies": [
		"sampctl/samp-stdlib",
		"pawn-lang/YSI-Includes:v5.1",
		"Southclaws/pawn-requests",
		"Awsomedude/easyDialog:2.0",
		"BigETI/pawn-map",
		"AbyssMorgan/MD5"
	],
	"builds": [
		{
			"name": "dev",
			"args": [
				"-;+",
				"-(+",
				"-d3",
				"-Z+"
			],
			"constants": {
				"MAX_PLAYERS": "20"
			}
		},
		{
			"name": "prod",
			"args": [
				"-;+",
				"-(+",
				"-O2",
				"-Z+"
			],
			"constants": {
				"MAX_PLAYERS": "300"
			}
		}
	],
	"runtime": {
		"version": "0.3.7",
		"mode": "server",
		"echo": "-",
		"rcon_password": "password",
		"port": 7777,
		"hostname": "Papawy RP V8",
		"maxplayers": 50,
		"language": "",
		"mapname": "Los Santos",
		"weburl": "github.com/Papawy/papawy-rp-v8",
		"gamemodetext": "PapawyRPV8",
		"announce": true,
		"lanmode": false,
		"query": true,
		"rcon": false,
		"logqueries": false,
		"sleep": 5,
		"maxnpc": 0,
		"stream_rate": 1000,
		"stream_distance": 200,
		"onfoot_rate": 30,
		"incar_rate": 30,
		"weapon_rate": 30,
		"chatlogging": true,
		"timestamp": true,
		"messageholelimit": 3000,
		"messageslimit": 500,
		"ackslimit": 3000,
		"playertimeout": 10000,
		"minconnectiontime": 0,
		"lagcompmode": 1,
		"connseedtime": 300000,
		"db_logging": false,
		"db_log_queries": false,
		"conncookies": true,
		"cookielogging": false,
		"output": true,
        "plugins": [
            "Southclaws/pawn-requests",
            "BigETI/pawn-memory"
        ]
	}
}
